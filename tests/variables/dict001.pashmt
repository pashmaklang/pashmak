--test--
dictonary item can get value

--file--
$my_dict = {'first': 1}

println $my_dict

$my_dict['second'] = 2

println $my_dict

$my_dict['last'] = 100

println $my_dict

class First
    $dict = {'a': 'A'}
endclass

$f = ^ new First

println $f->dict

$f->dict['b'] = 'B'

println $f->dict

class Hello
    $list = ['a', 'b', 'c']
endclass

$f->hello = ^ new Hello

println $f->hello->list

$f->hello->list[1] = '000'

println $f->hello->list

$f->dict['hello'] = $f->hello

println $f->dict['hello']->list

$f->dict['hello']->list[0] = 'ZZZ'

println $f->dict['hello']->list[0]

--output--
"""{'first': 1}
{'first': 1, 'second': 2}
{'first': 1, 'second': 2, 'last': 100}
{'a': 'A'}
{'a': 'A', 'b': 'B'}
['a', 'b', 'c']
['a', '000', 'c']
['a', '000', 'c']
ZZZ
"""
